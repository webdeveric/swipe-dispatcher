{
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "globals": {
  },
  "env": {
    "browser": true,
    "node": false,
    "es6": true
  },
  "rules": {
    "no-console": 0,
    "quotes": [1, "single"],
    "eol-last": 0,
    "no-mixed-requires": 0,
    "no-underscore-dangle": 0,
    "strict": [0, "never"],
    "no-var": 0,
    "no-unused-vars": 2,
    "semi": [1, "always"],
    "no-use-before-define": 2,
    "no-undef": 2
  }
}
