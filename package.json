{
  "name": "swipe-dispatcher",
  "version": "0.1.1",
  "description": "Dispatch custom swipe events to DOM nodes.",
  "keywords": [
    "swipe",
    "dispatch",
    "event",
    "customEvent",
    "dispatchEvent"
  ],
  "private": true,
  "license": "none",
  "author": {
    "email": "eric@webdeveric.com",
    "name": "Eric King",
    "url": "http://webdeveric.com/"
  },
  "contributors": [],
  "homepage": "https://github.com/webdeveric/swipe-dispatcher",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webdeveric/swipe-dispatcher.git"
  },
  "bugs": {
    "url": "https://github.com/webdeveric/swipe-dispatcher/issues"
  },
  "engines": {
    "node": ">= 0.10.0"
  },
  "files": [
    "lib/",
    "src/"
  ],
  "main": "lib/swipe-dispatcher.cjs.js",
  "jsnext:main": "lib/swipe-dispatcher.module.js",
  "scripts": {
    "lint": "eslint --fix src",
    "doc": "jsdoc -c ./jsdoc.json",
    "test": "mocha",
    "build:workspace": "rimraf ./lib/ && mkdirp ./lib/",
    "build:cjs": "babel src -o lib/swipe-dispatcher.cjs.js --no-babelrc --presets es2015",
    "build:module": "babel src -o lib/swipe-dispatcher.module.js",
    "build": "npm run build:workspace && npm run build:cjs && npm run build:module",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "custom-event-polyfill": "^0.3",
    "perfnow": "^0.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.18.0",
    "eslint": "^3.10.2",
    "eslint-config-webdeveric": "^0.1",
    "jsdoc": "^3.4.3",
    "mkdirp": "^0.5.1",
    "mocha": "^3.1.2",
    "rimraf": "^2.5.4"
  }
}
